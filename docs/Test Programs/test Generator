class SimpleProgram {
	public int mul(int a, int b) {
        int result = 0;
        while (b > 0) {
            result += a;
            b--;
        }
        return result;
    }
    public static void main(String[] args) {
        int x = 10;

        for(int i=0;i<x;i++){
            System.out.println(mul(i,2));
        }
    }
}